{
  "getContacts": {
    "schema": {
      "method": "GET",
      "protocol": "https",
      "host": "<%= iparam.subdomain %>.freshdesk.com",
      "path": "/api/v2/contacts",
      "headers": {
        "Authorization": "Basic <%= encode(iparam.apiKey) %>",
        "Content-Type": "application/json"
      }
    },
    "options": {
      "retryDelay": 1000,
      "isOAuth": true
    }
  }
}
